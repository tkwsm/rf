<h1>Tattributes in  ( Project ID: <%= params[:project_id] %>)</h1>

<p><%= @msg %> The last Tattribute ID: <%= @maxtattrid %></p>

<table class="table">
  <tr>
    <th>ID</th>
    <th>Project ID</th>
    <th>Transcript ID</th>
    <th>G tag</th>
    <th>G value</th>
  </tr>
  <% @data_pages.each do |obj| %>
  <tr>
    <td><%= obj.id %></td>
    <td><%= obj.project_id %></td>
    <td><%= obj.transcriptid %></td>
    <td><%= obj.ttag %></td>
    <td><%= obj.tvalue %></td>
  </tr>
  <% end %>
</table>

<br>
<div><%= paginate @data_pages %></div>


