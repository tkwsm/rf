<h1>Transcripts in  ( Project ID: <%= params[:project_id] %>)</h1>

<p><%= @msg %> The last Trarnscript ID: <%= @maxtid %></p>

<table class="table">
  <tr>
    <th>ID</th>
    <th>Project ID</th>
    <th>SeqID</th>
    <th>T Source</th>
    <th>T Type</th>
    <th>T Start</th>
    <th>T End</th>
    <th>T Score</th>
    <th>T Strand</th>
    <th>T Phase</th>
    <th>TranscriptID</th>
    <th>ParentID</th>
  </tr>
  <% @data_pages.each do |obj| %>
  <tr>
    <td><%= obj.id %></td>
    <td><%= obj.project_id %></td>
    <td><%= obj.seqid %></td>
    <td><%= obj.gsource %></td>
    <td><%= obj.gtype %></td>
    <td><%= obj.gstart %></td>
    <td><%= obj.gend %></td>
    <td><%= obj.gscore %></td>
    <td><%= obj.gstrand %></td>
    <td><%= obj.gphase %></td>
    <td><%= obj.transcriptid %></td>
    <td><%= obj.parentid %></td>
  </tr>
  <% end %>
</table>

<br>
<div><%= paginate @data_pages %></div>
<br>

