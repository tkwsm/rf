<p id="notice"><%= notice %></p>

<h1>Protein Seqs</h1>

<p><%= @msg %> The last Protein Seq ID: <%= @maxprotein_seqid  %></p>
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Project</th>
      <th>Protein</th>
      <th>Prot sequence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @data_pages.each do |protein_seq| %>
      <tr>
        <td><%= protein_seq.id %></td>
        <td><%= protein_seq.project_id %></td>
        <td><%= protein_seq.proteinid %></td>
        <td><%= protein_seq.prot_sequence.size %></td>
        <td><%# = link_to 'Show', protein_seq %></td>
        <td><%# = link_to 'Edit', edit_protein_seq_path(protein_seq) %></td>
        <td><%# = link_to 'Destroy', protein_seq, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<br>
<div><%= paginate @data_pages %></div>
<br>

